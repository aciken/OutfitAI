{
  "expo": {
    "name": "OutfitAI",
    "slug": "OutfitAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/outfitaiIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/outfitaiIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aciken.OutfitAI"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/outfitaiIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/outfitaiIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow OutfitAI to access your photos so you can select images for your outfits and profile.",
          "cameraPermission": "Allow OutfitAI to access your camera so you can take photos for your outfits and profile.",
          "microphonePermission": "Allow OutfitAI to access your microphone if you intend to capture video with audio."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c4854875-8884-4251-a6ff-d4e526dccc79"
      }
    }
  }
}
